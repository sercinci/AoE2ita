{% extends "layout.html.twig" %}

{% block title %}{{tournament.title}} - AoE2ita{% endblock %}
{% block meta %}
    <meta name="description" content="{{tournament.description|striptags}}" />
    <meta property="og:url"           content="https://aoe2ita.tk/tournaments/{{tournament.id}}" />
    <meta property="og:title"         content="{{tournament.title}} - AoE2ita" />
    <meta property="og:description"   content="{{tournament.description|striptags}}" />
    <meta name="twitter:title" content="{{tournament.title}} - AoE2ita" />
    <meta name="twitter:description" content="{{tournament.description|striptags}}" />
    <meta http-equiv="refresh" content="0;url=/tournaments/{{tournament.id}}" />
{% endblock %}
{% block content %}
    
{% endblock %}
